		-->
		<script type="text/javascript" src="/javascripts/desktop/lib/date.js"></script>
		<script type="text/javascript" src="/javascripts/desktop/lib/Datejs-all/src/extras.js"></script>
		<script type="text/javascript">
var clientTime=new Date();

		</script>
		<script type="text/javascript" src="/javascripts/desktop/less-1.1.4.min.js"></script>
		<script type="text/javascript" src="http://maps.google.com/maps/api/js?libraries=places&amp;sensor=true"></script>
		<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js"></script>
		<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.min.js"></script>
		<script type="text/javascript" src="/javascripts/jquery_ujs.js"></script>
		<script type="text/javascript" src="/javascripts/desktop/lib/cubiq-iscroll-8165866/src/iscroll.js"></script>
