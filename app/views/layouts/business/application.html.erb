   <body class="homepage">
      <div class="navbar navbar-fixed-top">
         <div class="navbar-inner">
            <div class="container">
               <a class="brand" href="/">&#160;</a>
               <ul class="nav pull-right">
                  <% if signed_in? %>
                  <% if current_merchant.status == :active %>
                  <li class=""><a href="/dashboard">Dashboard</a></li>
                  <li class=""><a href="/marketing">Marketing</a></li>
                  <li class=""><a href="/analytics">Analytics</a></li>
                  <li class=""><a href="/customer_rewards">Rewards</a></li>
                  <li class=""><a href="/challenges">Challenges</a></li>
                  <li class=""><a href="/venues">Venues</a></li>
                  <% end %>
                  <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Account <b class="caret"></b></a>
                     <ul class="dropdown-menu">
                        <li><a href="/account">Settings</a></li>
                        <li class="divider"></li>
                        <li><a href="/billings">Billings</a></li>
                     </ul></li>
                  <li class=""><a href="/sign_out">Sign Out</a></li>
                  <% end %>
               </ul>
            </div>
         </div>
      </div>
      <!--/container-->
      <div name="top" class="container-fluid">
         <div style="margin-top:100px;">
            &#160;
         </div>
         <div class="clearfix"></div>
         <div class="alert-message warning fade hide">
            <a class="close">x</a>
         </div>
