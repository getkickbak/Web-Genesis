         <div class="navbar-inner">
            <div class="container">
               <ul class="nav pull-left">
                  <li><a href="/">&copy; Copyright KICKBAK 2012</a></li>
               </ul>
               <ul class="nav pull-right">
                  <% if signed_in? %> <li><a href="/merchant_terms">Terms</a></li><% end %> <li><a href="/contact_us">Contact Us</a></li>
               </ul>
            </div>
         </div>
      </div>
