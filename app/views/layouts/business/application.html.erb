		<script type="text/javascript" src="/javascripts/desktop/lib/twitter-bootstrap-e4993fc/js/bootstrap-alerts.js"></script>
		<script type="text/javascript" src="/javascripts/desktop/lib/twitter-bootstrap-e4993fc/js/bootstrap-dropdown.js"></script>
