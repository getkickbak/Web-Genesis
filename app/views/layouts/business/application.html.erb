		<script type="text/javascript" src="/javascripts/desktop/src/common.js"></script>
		<%= yield :script %>
		<%= csrf_meta_tag %>
	</head>
	<body>
		<div id="topbar" class="topbar">
			<div class="topbar-inner noWrap">
				<div class="container">
					<h3><a href="/"><img width="200" style="position:absolute;top:0px;left:-10px;" src="http://photos.justformyfriends.com/logo.png"/>JustForMyFriends</a></h3>
					<ul class="nav secondary-nav">
						<% if signed_in? %>
						<% if current_merchant.status == :active %>
						<li class="">
							<a href="/dashboard">Dashboard</a>
						</li>
						<li class="">
							<a href="/marketing">Marketing</a>
						</li>
						<li class="">
							<a href="/analytics">Analytics</a>
						</li>
						<li class="menu" data-dropdown="dropdown">
							<div style="display:none;float:left;"></div><a href="#" class="menu" style="float:left;">Rewards</a>
							<ul class="menu-dropdown">
								<li>
									<a href="/purchase_rewards">Purchase Rewards</a>
								</li>
								<li class="divider"></li>
								<li>
									<a href="/customer_rewards">Customer Rewards</a>
								</li>
							</ul>
						</li>
						<li class="">
							<a href="/challenges">Challenges</a>
						</li>
						<li class="">
							<a href="/venues">Venues</a>
						</li>
						<% end %>
						<li class="menu" data-dropdown="dropdown">
							<div style="display:none;float:left;"></div><a href="#" class="menu" style="float:left;">Account</a>
							<ul class="menu-dropdown">
								<li>
									<a href="/account">Settings</a>
								</li>
								<li class="divider"></li>
								<li>
									<a href="/billings">Billings</a>
								</li>
							</ul>
						</li>
						<li class="">
							<a href="/sign_out">Sign Out</a>
						</li>
						<% end %>
					</ul>
				</div>
			</div>
		</div>
		<div name="top" class="container">
			<div style="margin-top:100px;" >
				&#160;
			</div>
			<div class="clearfix"></div>
			<div class="alert-message warning fade hide">
				<a class="close">x</a>
			</div>
			<div class="alert-message error fade hide">
				<a class="close">x</a>
			</div>
			<%= yield %> <!-- Footer  -->
			<footer>
				<div class="right">
					<% if signed_in? %> <a href="/merchant_terms">terms</a> |
					<% end %> <a href="/contact_us">contact us</a>
				</div>
				<p>
					&copy;JustForMyFriends 2011
				</p>
			</footer>
