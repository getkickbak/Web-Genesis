<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<meta http-equiv="Content-type" content="text/html; charset=utf-8">
		<%= yield :meta %>
		<!-- Le styles -->
		<link href="/javascripts/main_desktop/css/bootstrap.css" rel="stylesheet">
		<link href="/javascripts/main_desktop/css/style.css" rel="stylesheet">
		<link rel='stylesheet' id='prettyphoto-css'  href="/javascripts/main_desktop/css/prettyPhoto.css" type='text/css' media='all'>

		<style>
			body {
				padding-top:70px; /* 60px to make the container go all the way to the bottom of the topbar */
			}
		</style>

		<!-- Google Fonts call. Font Used Open Sans & Open Sans Condensed -->
		<link href='http://fonts.googleapis.com/css?family=Open+Sans+Condensed:300,700' rel='stylesheet' type='text/css'>
		<link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>

		<link href="/javascripts/main_desktop/css/bootstrap-responsive.css" rel="stylesheet">

		<!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
		<!--[if lt IE 9]>
		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
		<![endif]-->

		<!-- Le fav and touch icons -->
		<link rel="shortcut icon" href="https://s3.amazonaws.com/files.getkickbak.com/v1/favicon.jpg">
		<link rel="apple-touch-icon-precomposed" sizes="144x144" href="/javascripts/main_desktop/ico/apple-touch-icon-144-precomposed.png">
		<link rel="apple-touch-icon-precomposed" sizes="114x114" href="/javascripts/main_desktop/ico/apple-touch-icon-114-precomposed.png">
		<link rel="apple-touch-icon-precomposed" sizes="72x72" href="/javascripts/main_desktop/ico/apple-touch-icon-72-precomposed.png">
		<link rel="apple-touch-icon-precomposed" href="/javascripts/main_desktop/ico/apple-touch-icon-57-precomposed.png">

		<!-- JQuery must init first for prettyPhoto script -->
		<script type="text/javascript" src="/javascripts/main_desktop/js/jquery.js"></script>

		<!-- prettyPhoto Initialization -->
		<script type="text/javascript" charset="utf-8">
			$(document).ready(function() {
			$("a[rel^='prettyPhoto']").prettyPhoto();
			});
		</script>
		<%= yield :link %>
		<%= csrf_meta_tag %>
	</head>
	<body>

		<div class="navbar navbar-inverse navbar-fixed-top">
			<div class="navbar-inner">
				<div class="container">
					<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse"> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </a>
					<a class="brand" href="/"><img src="/javascripts/main_desktop/images/logo.png" alt="KickBak"></a>
					<div class="nav-collapse collapse">
						<ul class="nav">
							<li class="dropdown">
								<a href="/">Home</a>
							</li>
							<li class="dropdown">
								<a href="/about_us">About Us</a>
							</li>
							<li class="dropdown">
								<a href="/business">For Business</a>
							</li>
							<!--
							<li class="dropdown">
								<a href="/find_locations">Find Locations</a>
							</li>
							-->
							<li class="nav-sign-up">
								<a href="/sign_in">Login</a>
							</li>
							<li class="nav-sign-up">
								<a href="/sign_up">Sign-Up</a>
							</li>
						</ul>
					</div><!--/.nav-collapse -->
				</div>
			</div>
		</div>

		<div name="top" class="container">
			<div class="alert alert-warning fade hide">
				<a class="close">x</a>
			</div>
			<div class="alert alert-error fade hide">
				<a class="close">x</a>
			</div>
		</div>
		<%= yield %>

		<div id="footerwrap">
			<p class="footer-links">
				<a href="/about_us" title="About Us" rel="nofollow">About Us</a>
				<a href="/business" title="For Business" rel="nofollow">For Business</a>
				<!--<a href="/find_locations" title="Find Locations" rel="nofollow">Find Locations</a>-->
				<!--<a href="/faq" title="FAQ" rel="nofollow">FAQ</a>-->
				<!--<a href="/blog" title="Blog" rel="nofollow">Blog</a>-->
				<a href="/privacy" title="Privacy" rel="nofollow">Privacy</a>
				<a href="/terms" title="Terms" rel="nofollow">Terms</a>
				<a href="/contact_us" title="Contact Us" rel="nofollow">Contact Us</a>
			</p>
