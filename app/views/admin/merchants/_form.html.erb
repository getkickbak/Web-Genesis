<%= simple_form_for [@merchant] do |f| %>
	<%= f.input :name %>
	<%= f.input :email %>
	<%= f.input :account_first_name %>
	<%= f.input :account_last_name %>
	<%= f.input :password %>
	<%= f.input :password_confirmation %>
	<%= f.input :phone %>
	<%= f.input :status %>
	<%= f.button :submit %>
<% end %>
