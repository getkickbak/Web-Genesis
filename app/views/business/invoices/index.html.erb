<% content_for :link do %>
<link rel="stylesheet" href="/javascripts/desktop/css/add_business.css"/>
<!--[if lt IE 9]>
<link rel="stylesheet" href="/javascripts/desktop/css/add_business-ie8.css"/>
<![endif]-->
<% end %>
<div class="row-fluid">
	<div class="span3"></div>
	<div class="span6">
		<ul class="nav nav-list well">
			<li class="nav-header">
				Listing Invoices
			</li>
			<% @invoices.each do |invoice| %>
			<div>
				<%= link_to invoice.created_ts.strftime("%b %Y"), invoice_path(invoice) %>
			</div>
			<% end %>
		</ul>
	</div>
</div>
