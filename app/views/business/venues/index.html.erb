<h1>Listing Venues</h1>

<table>
	<tr>
		<th>Name</th>
		<th>Address1</th>
		<th>City</th>
		<th>State</th>
		<th>ZipCode</th>
		<th>Country</th>
		<th>Phone</th>
		<th>Website</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>

	<% @venues.each do |venue| %>
	<tr>
		<td><%= venue.name %></td>
		<td><%= venue.address1 %></td>
		<td><%= venue.city %></td>
		<td><%= venue.state %></td>
	    <td><%= venue.zipcode %></td>
	    <td><%= venue.country %></td>
	    <td><%= venue.phone %></td>
	    <td><%= venue.website %></td>
		<td><%= link_to 'Show', venue_path(venue) %></td>
		<td><%= link_to 'Edit', edit_venue_path(venue) %></td>
		<td><%= link_to 'Destroy', venue_path(venue), :confirm => 'Are you sure?', :method => :delete %></td>
	</tr>
	<% end %>
</table>

<br />

<%= link_to 'New Venue', new_venue_path %>
